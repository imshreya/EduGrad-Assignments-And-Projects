    "def sel_sort(a):\n",
    "    for i in range(0,len(a)):\n",
    "        for j in range(i+1,len(a)):\n",
    "            if(a[i]>a[j]):\n",
    "                t=a[i]\n",
    "                a[i]=a[j]\n",
    "                a[j]=t\n",
    "    return a\n",
    "a=[80,70,90,40,60,50]\n",
    "print(\"Sorted Array is :\",sel_sort(a))"
   ]
  }
